    "description": "Cloakings Palladium Client",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": ">=8.1",
        "ext-curl": "*",
        "cloakings/cloakings-common": "^1.1",
        "gupalo/json": "^1.0",
        "psr/log": "^3.0",
        "symfony/http-client": "^6.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Cloakings\\CloakingsPalladium\\": "src/CloakingsPalladium/"
